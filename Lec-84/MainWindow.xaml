        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lec_84"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Add" HorizontalAlignment="Left" Height="40" Margin="168,132,0,0" VerticalAlignment="Top" Width="121" Click="AddBtn"/>
        <Button Content="Remove" HorizontalAlignment="Left" Height="40" Margin="318,132,0,0" VerticalAlignment="Top" Width="121" Click="RemoveBtn"/>
        <Button Content="Show All" HorizontalAlignment="Left" Height="40" Margin="471,132,0,0" VerticalAlignment="Top" Width="121" Click="ShowallBtn"/>
        <TextBox x:Name="Valuetxt" HorizontalAlignment="Left" Height="28" Margin="168,80,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="424"/>
        <TextBox x:Name="Keytxt" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="423" Margin="169,44,0,0"/>

    </Grid>
</Window>
